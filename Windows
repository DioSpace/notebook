Windows

//延迟关机
shutdown -s -t 100
//取消延迟关机
shutdown /a


//批处理文件
xxx.bat


//查看所有port
netstat -ano
//查看并过虑port
netstat -ano | findstr 5200
//开放port
telnet 134.175.224.245 5200
#防火墙开放3306端口
netsh advfirewall firewall add rule name=mysql dir=in action=allow protocol=TCP localport=3306
#防火墙删除3306端口
netsh advfirewall firewall delete rule name=mysql protocol=tcp localport=3306


//查看并过虑进程
ps | findstr 1920


//杀进程
kill 12308